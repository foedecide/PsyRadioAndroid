<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="dashboard_button_container">
        <item name="android:orientation">vertical</item>
        <!-- <item name="android:background">@drawable/dashboard_button_bg</item> -->
        <item name="android:focusable">true</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:selectable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="dashboard_image_container">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="dashboard_item_image">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="dashboard_item_label">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>